    "def fibo(n,dict_):\n",
    "    global memo\n",
    "    memo = dict_\n",
    "    if n==1 or n==2: \n",
    "        return 1\n",
    "    else:\n",
    "        if not n in memo:\n",
    "            memo[n]= fibo(n-1, memo) + fibo(n-2,memo)\n",
    "    return memo[n]\n",
    "        \n",
    "dict_={}\n",
    "fibo(118,dict_)"
