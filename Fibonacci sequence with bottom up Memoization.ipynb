    "def cost(row, col, mat):\n",
    "#     cost= 0\n",
    "    if col< 0 or row< 0:\n",
    "        return float('inf')\n",
    "    if col== 0 and row == 0:\n",
    "        return mat[row][col]\n",
    "    else:\n",
    "        min1= cost(row-1, col, mat)\n",
    "        min2= cost(row, col-1,mat)\n",
    "        \n",
    "        return mat[row][col]+min(min1,min2)"
   ]
